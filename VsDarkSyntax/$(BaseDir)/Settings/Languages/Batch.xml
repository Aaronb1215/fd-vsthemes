<?xml version="1.0" encoding="utf-8"?>
<Scintilla xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<languages>
		<language name="batch">
			<lexer name="batch" style-bits="7" />
			<file-extensions>bat,cmd</file-extensions>
			<!-- COLORING_START -->
			<editor-style caret-fore="0xF1F1F1" caretline-back="0x0F0F0F" selection-fore="0xDCDCDC" selection-back="0x264F78" margin-fore="0x1E1E1E" margin-back="0x1E1E1E" marker-fore="0x1E1E1E" colorize-marker-back="true" />
			<use-styles>
				<style name="default" fore="0xDCDCDC" font="Consolas" size="10" back="0x1E1E1E" bold="false" />
				<style name="comment" fore="0x57A63A" />
				<style name="word" />
				<style name="label" fore="0x45C9B0" eolfilled="true" />
				<style name="hide" fore="0x808080" />
				<style name="command" fore="0x429CD6" />
				<style name="identifier" fore="0xD69D85" />
				<style name="operator" fore="0xB4B4B4" />
				<style name="gdefault" fore="0x333333" />
				<style name="linenumber" fore="0x666666" />
				<style name="bracelight" back="0x303030" />
				<style name="bracebad" />
				<style name="controlchar" />
				<style name="indentguide" fore="0xc0c0c0" />
				<style name="lastpredefined" />
			</use-styles>
			<!-- COLORING_END -->
		</language>
	</languages>
</Scintilla>