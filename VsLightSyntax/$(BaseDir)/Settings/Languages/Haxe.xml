<?xml version="1.0" encoding="utf-8"?>
<Scintilla xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<keyword-classes>
		<keyword-class name="haxe-primary-keywords">
			abstract break case catch class continue default do else enum extends for function if implements import in interface macro new package return switch throw try typedef using var while $type 
		</keyword-class>
		<keyword-class name="haxe-secondary-keywords">
			null true false
		</keyword-class>
		<keyword-class name="haxe-additional-keywords">
			Void Int Float Dynamic Bool UInt Iterator Array List Hash IntHash Date String Xml
		</keyword-class>
		<keyword-class name="haxe-documentation-keywords">
			author copy default deprecated eventType example exampleText exception haxe inheritDoc internal link
			param private return see serial serialData serialField since throws usage version
		</keyword-class>
		<keyword-class name="haxe-additional-keywords3">
			dynamic extern inline override private public static
		</keyword-class>
		<keyword-class name="haxe-additional-keywords4">
			untyped cast trace
		</keyword-class>
		<keyword-class name="haxe-additional-keywords5">
			super this arguments
		</keyword-class>
	</keyword-classes>
	<languages>
		<language name="haxe">
			<lexer name="cpp" style-bits="7" />
			<character-class><![CDATA[abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ0123456789_]]></character-class>
			<line-comment><![CDATA[//]]></line-comment>
			<comment-start><![CDATA[/*]]></comment-start>
			<comment-end><![CDATA[*/]]></comment-end>
			<file-extensions>hx</file-extensions>
			<use-keywords>
				<keyword key="0" class="haxe-secondary-keywords" />
				<keyword key="1" class="haxe-additional-keywords" />
				<keyword key="2" class="haxe-documentation-keywords" />
				<keyword key="3" class="haxe-primary-keywords" />
				<keyword key="5" class="haxe-additional-keywords3" />
				<keyword key="6" class="haxe-additional-keywords4" />
				<keyword key="7" class="haxe-additional-keywords5" />
			</use-keywords>
			<!-- COLORING_START -->
			<editor-style caret-fore="0x000000" caretline-back="0xECECEC" selection-back="0xADD6FF" selection-fore="0x000000" margin-fore="0xFFFFFF" margin-back="0xFFFFFF" marker-back="0xB6B6B6" colorize-marker-back="true" />
			<use-styles>
				<style name="default" fore="0x000000" back="0xFFFFFF" font="Consolas" size="10" bold="false" />
				<style name="comment" fore="0x008000" />
				<style name="commentline" fore="0x008000" />
				<style name="commentdoc" fore="0x008000" />
				<style name="number" fore="0x000000" />
				<style name="word" fore="0x0000FF" />
				<style name="string" fore="0xA31515" />
				<style name="character" fore="0xA31515" />
				<style name="uuid" />
				<style name="preprocessor" />
				<style name="operator" />
				<style name="identifier" />
				<style name="stringeol" />
				<style name="verbatim" />
				<style name="regex" fore="0x800000" />
				<style name="commentlinedoc" fore="0x008000" />
				<style name="word2" fore="0x2B91AF" />
				<style name="word3" fore="0x0000ff" />
				<style name="word4" />
				<style name="word5" fore="0x0000FF" />
				<style name="commentdockeyword" fore="0x808080" />
				<style name="commentdockeyworderror" fore="0x800000" />
				<style name="globalclass" fore="0x1518ff" />
				<style name="gdefault" fore="0xE6E7E8" />
				<style name="linenumber" fore="0x2B91AF" />
				<style name="bracelight" back="0xE2E6D6" />
				<style name="bracebad" />
				<style name="controlchar" />
				<style name="indentguide" fore="0xB6B6B6" />
				<style name="lastpredefined" />
			</use-styles>
			<!-- COLORING_END -->
		</language>
	</languages>
</Scintilla>